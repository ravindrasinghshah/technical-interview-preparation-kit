{
  "name": "dsalgo",
  "version": "1.0.0",
  "description": "DS Algo Exercise - TypeScript scaffolding for sorting algorithms",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "demo": "tsx demo/bubbleSort-demo.ts",
    "demo:js": "node demo/bubbleSort-demo.js",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "clean": "rimraf dist",
    "start": "node dist/index.js"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "vitest": "^1.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "tsx": "^4.0.0",
    "@types/node": "^20.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0"
  },
  "keywords": [
    "algorithms",
    "data-structures",
    "typescript",
    "sorting",
    "bubble-sort",
    "computer-science"
  ],
  "author": "Ravindra Singh Shah",
  "license": "ISC",
  "engines": {
    "node": ">=18.0.0"
  }
}
